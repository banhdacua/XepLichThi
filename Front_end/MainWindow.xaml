<Window x:Class="XepLichThi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:XepLichThi"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid RenderTransformOrigin="0.504,0.412">
        <DataGrid x:Name="grwDanhSach" d:ItemsSource="{d:SampleData ItemCount=5}" Margin="0,121,0,0" RenderTransformOrigin="0.5,0.698"/>
        <Button x:Name="btnLoadFile" Content="Load File" HorizontalAlignment="Left" Margin="89,38,0,0" VerticalAlignment="Top" RenderTransformOrigin="2.097,-3.51" Height="34" Width="76" Click="btnLoadFile_Click"/>
        <Button x:Name="btnSchedule" Content="Xếp Lịch" HorizontalAlignment="Left" Margin="244,38,0,0" VerticalAlignment="Top" Height="34" Width="67" Click="btnSchedule_Click"/>

    </Grid>
</Window>
